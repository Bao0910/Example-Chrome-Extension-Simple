{
  "manifest_version": 2,
  "name": "Super CTMS",
  "description": "This extension description",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://ctms.fithou.net.vn/"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}